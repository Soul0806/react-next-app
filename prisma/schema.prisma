// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Specification {
  id     Int    @id @default(autoincrement())
  format String
}

model Record {
  id        Int      @id @default(autoincrement())
  area      String
  service   String
  spec      String
  quantity  String
  price     String
  pay       String
  note      String
  date      String
  createdAt DateTime @default(now())
}
